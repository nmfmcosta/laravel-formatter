{
    "name": "nmfmcosta/laravel-formatter",
    "type": "library",
    "description": "A formatting library that converts data output between XML, CSV, JSON, TXT, YAML and a few others.",
    "keywords": ["laravel", "formatter", "data", "convert", "csv", "xml", "yaml"],
    "homepage": "http://github.com/nmfmcosta/laravel-formatter",
    "license": "MIT",
    "version": "1.0",
    "require": {
        "php": "^7.2|^8.0",
        "league/csv": "~9.0",
        "mustangostang/spyc": "~0.6",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "LB\\Formatter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Formatter": "LB\\Formatter"
            }
        }
    },
    "minimum-stability": "stable"
}
